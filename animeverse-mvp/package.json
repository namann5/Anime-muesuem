  },
  "dependencies": {
    "@react-three/drei": "^9.96.1",
    "@react-three/fiber": "^8.15.12",
    "@uiball/loaders": "^1.3.1",
    "firebase": "^12.7.0",
    "gsap": "^3.12.0",
    "hls.js": "^1.6.15",
    "howler": "^2.2.3",
    "qrcode.react": "^4.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-lottie": "^1.2.10",
    "three": "^0.160.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.0",
    "vite": "^5.2.0",
    "prettier": "^2.8.8",
    "concurrently": "^8.2.0"
  }
}
