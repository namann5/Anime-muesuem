{
  "name": "animeverse-mvp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,md,json}\" || true",
    "dev:client": "vite",
    "dev:server": "cd server && npm start",
    "dev:consumet": "cd ../api.consumet.org && npm start",
